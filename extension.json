  "author": {
    "name": "Algolia"
  },
  "viewBasePath": "dist/",
  "iconPath": "resources/icons/algolia.svg",
  "configuration": {
    "viewPath": "configuration/configuration.html",
    "schema": {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "type": "object",
      "properties": {
        "appId": {
          "type": "string"
        },
        "apiKey": {
          "type": "string"
        },
        "indexName": {
          "type": "string"
        },
        "userTokenDataElement": {
          "type": "string"
        },
        "authenticatedUserTokenDataElement": {
          "type": "string"
        },
        "currency": {
          "type": "string"
        }
      }
    }
  },
  "actions": [
    {
      "displayName": "Load Insights",
      "name": "load-insights",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {
          "useUserTokenCookie": {
            "type": "boolean"
          },
          "version": {
            "type": "string"
          },
          "userHasOptedOutDataElement": {
            "type": "string"
          }
        }
      },
      "libPath": "src/lib/actions/loadInsights.js",
      "viewPath": "actions/loadInsights.html"
    },
    {
      "displayName": "Viewed",
      "name": "viewed",
      "schema": {
