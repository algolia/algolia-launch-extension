{
  "displayName": "Algolia Search Insights",
  "name": "algolia",
  "platform": "web",
  "version": "2.1.8",
  "description": "Send events to Algolia for AI features, e.g. personalization and recommendation.",
  "author": {
    "name": "Algolia"
  },
  "viewBasePath": "dist/",
  "iconPath": "resources/icons/algolia.svg",
  "configuration": {
    "viewPath": "configuration/configuration.html",
    "schema": {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "type": "object",
      "properties": {
        "appId": {
          "type": "string"
        },
        "apiKey": {
          "type": "string"
        },
        "indexName": {
          "type": "string"
        },
        "useUserTokenCookie": {
          "type": "boolean"
        },
        "version": {
          "type": "string"
        },
        "userHasOptedOutDataElement": {
          "type": "string"
        }
      }
    }
  },
  "actions": [
    {
      "displayName": "Load Insights",
      "name": "load-insights",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {}
      },
      "libPath": "src/lib/actions/loadInsights.js"
    },
    {
      "displayName": "Viewed",
      "name": "viewed",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {
          "eventDetailsDataElement": {
            "type": "string"
          },
          "userTokenDataElement": {
            "type": "string"
          },
          "eventName": {
            "type": "string"
          }
        }
      },
      "libPath": "src/lib/actions/viewed.js",
      "viewPath": "actions/viewed.html"
    },
    {
      "displayName": "Converted",
      "name": "converted",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {
          "eventDetailsDataElement": {
            "type": "string"
          },
          "userTokenDataElement": {
            "type": "string"
          },
          "eventName": {
            "type": "string"
          }
        }
      },
      "libPath": "src/lib/actions/converted.js",
      "viewPath": "actions/converted.html"
    },
    {
      "displayName": "Clicked",
      "name": "clicked",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {
          "eventDetailsDataElement": {
            "type": "string"
          },
          "userTokenDataElement": {
            "type": "string"
          },
          "eventName": {
            "type": "string"
          }
        }
      },
      "libPath": "src/lib/actions/clicked.js",
      "viewPath": "actions/clicked.html"
    }
  ],
  "dataElements": [
    {
      "displayName": "DataSet",
      "name": "dataset",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {
          "hitQuerySelector": {
            "type": "string"
          },
          "indexNameQuerySelector": {
            "type": "string"
          }
        }
      },
      "libPath": "src/lib/dataElements/dataset.js",
      "viewPath": "dataElements/dataset.html"
    },
    {
      "displayName": "Query String",
      "name": "query-string",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {
          "queryIDParamName": {
            "type": "string"
          },
          "indexNameParamName": {
            "type": "string"
          },
          "objectIDParamName": {
            "type": "string"
          },
          "positionParamName": {
            "type": "string"
          }
        }
      },
      "libPath": "src/lib/dataElements/queryString.js",
      "viewPath": "dataElements/queryString.html"
    },
    {
      "displayName": "Storage",
      "name": "storage",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {
        }
      },
      "libPath": "src/lib/dataElements/storage.js"
    }
  ]
}
