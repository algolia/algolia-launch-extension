{
  "name": "algolia-insights-extension",
  "version": "2.0.3",
  "author": {
    "name": "Algolia",
    "url": "https://www.algolia.com/"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "turbine",
    "extension",
    "radar"
  ],
  "scripts": {
    "build": "rm -rf dist && webpack --mode production",
    "build:watch": "webpack --mode development --watch",
    "sandbox": "concurrently -k -n webpack,sandbox -c \"bgBlue,bgMagenta\" \"npm run build:watch\" \"reactor-sandbox\"",
    "package": "npm run build && npx @adobe/reactor-packager",
    "prepack": "npm test && npm run build",
    "lint": "eslint 'src/**'",
    "format": "pretty-quick",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint -- --fix"
    ]
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.23.0",
    "@adobe/reactor-document": "^1.0.0",
    "@adobe/reactor-window": "^1.0.0",
    "@spectrum-icons/illustrations": "^3.5.1",
    "@spectrum-icons/workflow": "^4.0.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.40.0"
  },
  "devDependencies": {
    "@adobe/reactor-sandbox": "^12.5.0",
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.20.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "babel-polyfill": "^6.26.0",
    "camelcase": "^6.3.0",
    "capitalize": "^2.0.4",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.2",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-runner-eslint": "^1.1.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "style-loader": "^3.3.1",
    "stylus": "^0.59.0",
    "stylus-loader": "^7.1.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-shell-plugin": "^0.5.0",
    "yargs": "^17.6.2"
  }
}
